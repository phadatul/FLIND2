
public class Demo {
	public static void main(String args[]) {
		Employee e = new Employee("KAPIL",1200000, 13000);
		e.printEmp();
		Employee e1 = new Employee("KIRTI",1200000, 13000);
		e1.printEmp();
		Employee e3 = new Employee("TAPIL",1200000, 13000);
		e3.printEmp();
		System.out.println("ID: "+ Employee.empid);
	}
}
